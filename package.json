{
  "author": "Jon Cursi <jon.cursi@act-et.org>",
  "bugs": {
    "email": "jon.cursi@act-et.org",
    "url": "https://bitbucket.org/actet/eslint-config/issues"
  },
  "contributors": [
    "Jon Cursi <jon.cursi@act-et.org>"
  ],
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-disable": "^1.0.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-new-with-error": "^2.0.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-security": "^1.4.0",
    "markdownlint-cli": "^0.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "description": "⚙️ Our preferred configs for CommitLint, ESLint, MarkdownLint, Prettier, and TypeScript.",
  "devDependencies": {
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "homepage": "https://bitbucket.org/actet/eslint-config#readme",
  "keywords": [
    "commitlint",
    "eslint",
    "eslint-config",
    "markdownlint",
    "prettier",
    "typescript"
  ],
  "lint-staged": {
    "*.{js}": "npm run test:lint:js",
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{md}": "npm run test:lint:md"
  },
  "main": "index.js",
  "name": "eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/actet/eslint-config.git"
  },
  "scripts": {
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --quiet",
    "test:lint:md": "markdownlint README.md --config markdownlint.config.json"
  },
  "version": "1.0.0"
}
